# Dicionário: estrutura chave: valor
notas = {'João' : 6.0, "Maria": 8.0, 'Pedro': 6.5}
print(notas)
print(notas["João"])


# as chaves
print(notas.keys())
# Os valores
print(notas.values())


# testar a existência
print('João' in notas)
print('Fernando' in notas)


# Apaga elementos
del notas['João']
print('João' in notas)
print(notas)


# Incluir elemento
notas['Ana'] = 9.0
print(notas)


# Busca um valor, se não encontrar, pega o segundo parâmetro
notas.get("Geraldo","Não encontrado")


